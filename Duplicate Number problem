/*Duplicate Number problem. 
Given an array of n+1 integers where each integer is between 1 and n (inclusive), 
there is exactly one duplicate number. Find the duplicate number using only one for loop.*/
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a[]={1, 3, 4, 2, 2};
		int n=a.length-1;
		int ap=n*(n+1)/2;
		int sum=0;
		for(int i=0;i<=n;i++){
		   sum+=a[i];
		}
		System.out.println(sum-ap);
	}
}
